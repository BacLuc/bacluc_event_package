{
  "require-dev": {
    "friendsofphp/php-cs-fixer": "v3.4.0",
    "php": ">=7"
  },
  "autoload": {
    "psr-4": {
      "BaclucEventPackage\\": "src"
    }
  },
  "scripts": {
    "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --config .php_cs-fixer.php .",
    "cs-fix": "vendor/bin/php-cs-fixer fix --config .php_cs-fixer.php ."
  }
}
